{
  "name": "meshhub-altv-integration",
  "version": "1.0.0",
  "description": "WebView инструмент для ALT:V с интеграцией в MeshHub Backend для управления автомобилями",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node -e \"const fs=require('fs'),path=require('path');const assetsPath=path.resolve('..','altv-server','resources','meshhub','client','assets');if(fs.existsSync(assetsPath)){fs.rmSync(assetsPath,{recursive:true,force:true});console.log('Cleaned client/assets/');}\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "build-webview": "./scripts/build-webview.sh",
    "build-resource": "./scripts/build-resource.sh",
    "prebuild:fivem": "node -e \"const fs=require('fs'),path=require('path');const assetsPath=path.resolve('..','FIVEM_2','txData','CFXDefault_6E2ECF.base','resources','meshhub-fivem','html','assets');if(fs.existsSync(assetsPath)){fs.rmSync(assetsPath,{recursive:true,force:true});console.log('Cleaned FiveM html/assets/');}\"",
    "build:fivem": "tsc -b && vite build --config vite.config.fivem.ts"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.453.0",
    "pako": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.3.0",
    "three": "^0.158.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/crypto-js": "^4.2.0",
    "@types/node": "^24.9.1",
    "@types/pako": "^2.0.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.4",
    "vite": "^5.4.5"
  },
  "keywords": [
    "altv",
    "meshhub",
    "webview",
    "vehicle",
    "handling"
  ],
  "author": "MeshHub Team",
  "license": "MIT"
}
